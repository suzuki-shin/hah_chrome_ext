{
    "name": "hah",
    "description" : "hit a hint and tab switcher",
    "version": "0.0.4",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "exclude_matches": [
                "*://mail.google.com/*",
                "*://twitter.com/*"
                "*://facebook.com/*"
            ],
            "css": ["hitahint.css"],
            "js": ["jquery-1.9.0.min.js", "prelude.min.js", "hitahint.js"],
            "run_at": "document_end",
            "all_frames": false
        }
    ],
    "background": {
        "page": "background.html",
        "persistent": false
    },
    "permissions": [ "history", "tabs", "bookmarks" ],
    "manifest_version": 2
}
