{
    "name": "hah",
    "description" : "hit a hint and tab switcher",
    "version": "0.4.5",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "css": ["hitahint.css"],
            "js": [
                "jquery-1.9.0.min.js",
                "prelude.min.js",
                "keymap.js",
                "content_script.js"
            ],
            "all_frames": false
        }
    ],
    "background": {
        "page": "background.html",
        "persistent": true
    },
    "options_page": "options.html",
    "permissions": [ "history", "tabs", "bookmarks", "storage" ],
    "manifest_version": 2
}
